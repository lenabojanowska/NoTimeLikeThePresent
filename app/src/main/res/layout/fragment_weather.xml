<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A1673AB7"
    tools:context="View.Fragment.WeatherFragment">

    <androidx.cardview.widget.CardView
        android:layout_width="350dp"
        android:layout_height="500dp"
        app:cardCornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FFFFFF">

            <TextView
                android:id="@+id/textViewCity"
                android:layout_width="316dp"
                android:layout_height="47dp"
                android:layout_marginTop="36dp"
                android:text="CITY"
                android:textAlignment="center"
                android:textSize="30dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.529"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView3"
                android:layout_width="335dp"
                android:layout_height="131dp"
                app:cardBackgroundColor="#FFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewCity">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#F8FCFD">

                    <ImageView
                        android:id="@+id/imageViewWeather"
                        android:layout_width="111dp"
                        android:layout_height="107dp"
                        android:layout_marginStart="16dp"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.666"
                        tools:srcCompat="@tools:sample/avatars" />

                    <TextView
                        android:id="@+id/textViewTemperature"
                        android:layout_width="155dp"
                        android:layout_height="42dp"
                        android:layout_marginEnd="28dp"
                        android:text="TextView"
                        android:textAlignment="center"
                        android:textSize="30sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.617" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="316dp"
                android:layout_height="228dp"
                android:layout_marginTop="36dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardView3"
                app:layout_constraintVertical_bias="0.058">

                <TextView
                    android:id="@+id/textViewSth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="TextView"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.443"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textViewSth2" />

                <TextView
                    android:id="@+id/textViewSth2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Day"
                    android:textSize="24sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.458"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="122dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView13"
                    app:layout_constraintVertical_bias="1.0">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="168dp"
                            android:layout_height="match_parent"
                            android:background="#FFFFFF"
                            android:text="PRESSURE"
                            android:textColor="#070606" />

                        <TextView
                            android:id="@+id/textViewPressure"
                            android:layout_width="186dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="7dp"
                            android:text="PRESSURE"
                            android:textColor="#020202" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="WIND">

                        <TextView
                            android:layout_width="168dp"
                            android:layout_height="match_parent"
                            android:background="#FFFFFF"
                            android:text="HUMIDITY"
                            android:textColor="#070606" />

                        <TextView
                            android:id="@+id/textViewHumidity"
                            android:layout_width="186dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="7dp"
                            android:text="HUMIDITY"
                            android:textColor="#020202" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="168dp"
                            android:layout_height="match_parent"
                            android:background="#FFFFFF"
                            android:text="SUNRISE"
                            android:textColor="#070606" />

                        <TextView
                            android:id="@+id/textViewSunrise"
                            android:layout_width="186dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="7dp"
                            android:text="SUNRISE"
                            android:textColor="#020202" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="168dp"
                            android:layout_height="match_parent"
                            android:background="#FFFFFF"
                            android:text="SUNSET"
                            android:textColor="#070606" />

                        <TextView
                            android:id="@+id/textViewSunset"
                            android:layout_width="186dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="7dp"
                            android:text="SUNSET"
                            android:textColor="#020202" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="168dp"
                            android:layout_height="match_parent"
                            android:background="#FFFFFF"
                            android:text="COORDINATORS"
                            android:textColor="#070606" />

                        <TextView
                            android:id="@+id/textViewCoord"
                            android:layout_width="186dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="7dp"
                            android:text="COORDINATORS"
                            android:textColor="#020202" />
                    </TableRow>

                </TableLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>